  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "postinstall": "prisma generate --schema=./prisma/schema.prisma",
  "scripts": {
    "dev": "next dev --turbopack",
    "start:dev": "next dev --turbopack",
    "build": "NODE_OPTIONS='--max-old-space-size=512' prisma generate --schema=./prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test:connections": "tsx scripts/test-connections.ts",
    "setup:database": "tsx scripts/setup-database.ts",
    "test:env": "tsx -e \"require('dotenv').config(); import { validateEnvironment, logEnvironmentValidation } from './src/lib/env-validator'; logEnvironmentValidation();\"",
    "stop": "pkill -f 'next-server' 2>/dev/null || true; pkill -f 'npm run dev' 2>/dev/null || true; pkill -f 'yarn dev' 2>/dev/null || true; echo 'All development servers stopped'"
  },
